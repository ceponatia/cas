{
  "name": "@cas/types",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "pnpm run generate:zod && tsc",
    "dev": "tsc --watch",
    "generate:zod": "ts-to-zod --all --skipValidation",
    "dev:zod": "ts-to-zod --all --watch --skipValidation"
  },
  "devDependencies": {
    "ts-to-zod": "^3.15.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "zod": "^4.0.14"
  }
}
